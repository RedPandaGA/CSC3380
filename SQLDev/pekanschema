CREATE FUNCTION public."createPantryFunc"() RETURNS trigger
    LANGUAGE plpgsql
    AS $$BEGIN
	INSERT INTO pantry DEFAULT VALUES;
	RETURN NEW;
END;
$$;


ALTER FUNCTION public."createPantryFunc"() OWNER TO postgres;

--
-- TOC entry 218 (class 1255 OID 16439)
-- Name: updateUserPID(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public."updateUserPID"() RETURNS trigger
    LANGUAGE plpgsql
    AS $$BEGIN
	UPDATE users u SET "PID" = pantry."PID" FROM pantry WHERE pantry."PID" = u."UID";
	RETURN NULL;
END;
$$;


ALTER FUNCTION public."updateUserPID"() OWNER TO postgres;

